{
  "name": "app-datepicker",
  "version": "0.0.0",
  "private": true,
  "description": "Material Design based datepicker element built with Polymer",
  "keywords": [
    "app-datepicker",
    "node",
    "typescript"
  ],
  "homepage": "https://github.com/motss/app-datepicker",
  "license": "MIT",
  "author": {
    "name": "Rong Sen Ng",
    "email": "wes.ngrongsen@gmail.com",
    "url": "https://github.com/motss"
  },
  "main": "./index.js",
  "repository": {
    "type": "git",
    "url": "git@github.com:motss/app-datepicker.git"
  },
  "scripts": {
    "build": "npm run ts && cross-env NODE_ENV=production gulp build && npm run cleanup",
    "build:debug": "npm run ts && gulp build && npm run cleanup",
    "check": "npm run lint && npm run nsp",
    "cleanup": "rm -rf gulpfile.js",
    "gulp": "npm run ts && gulp",
    "lint": "npm run ts && gulp lint && npm run cleanup",
    "nsp": "nsp check",
    "prepublishOnly": "npm run check && npm run build",
    "ts": "tsc --target esnext --lib dom,dom.iterable,esnext,esnext.asynciterable --module esnext src/app-datepicker.ts"
  },
  "dependencies": {
    "@polymer/paper-button": "^3.0.0-pre.7",
    "@polymer/polymer": "^3.0.0-pre.7",
    "@webcomponents/webcomponentsjs": "^1.1.0"
  },
  "devDependencies": {
    "nsp": "^3.1.0",
    "tslint": "^5.9.1",
    "tslint-config-airbnb": "^5.5.0",
    "tslint-consistent-codestyle": "^1.11.0",
    "tslint-immutable": "^4.5.1",
    "typescript": "^2.6.2"
  },
  "engines": {
    "node": ">= 8.9.0",
    "npm": ">= 5.5.1"
  },
  "flat": true,
  "resolutions": {
    "chalk": "2.3.0",
    "ansi-styles": "3.2.0",
    "supports-color": "4.5.0",
    "lodash": "4.17.4",
    "string-width": "2.1.1",
    "is-fullwidth-code-point": "2.0.0",
    "strip-ansi": "4.0.0",
    "ansi-regex": "3.0.0",
    "tsutils": "2.20.0",
    "esutils": "2.0.2",
    "@polymer/polymer": "3.0.0-pre.7"
  }
}
